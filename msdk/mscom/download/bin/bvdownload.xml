		<productname>BvDownload</productname>
		<prouctid>{591B4CE9-EDF9-490C-AE55-C25C916E5DA3}</prouctid>
		<!--子产品ID-->
		<subproductname>{8F1473F5-79C3-4DE5-B8C5-FF165C35E42A}</subproductname>
		
